# S3  -*- sh -*-

s3_check() {
  s3cmd ls >/dev/null 2>&1
}

s3_install() {
  if s3_check ; then
    echo "S3 is already installed"
  else
    ${PKG}_install s3cmd
    s3cmd  --configure
  fi
}

s3_uninstall() {
  rm -f ~/.s3cfg
  ${PKG}_uninstall s3cmd
}

s3_status() {
  s3cmd list
}

s3_stop() {
  time
}

s3_start() {
  time
}
