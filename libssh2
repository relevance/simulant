# libssh2 -*- sh -*-

libssh2_check() {
  ${PKG}_check libssh2
}

libssh2_uninstall() {
  ${PKG}_uninstall libssh2
}

libssh2_install() {
  if ${PKG}_check libssh2 ; then
    echo "libssh2 already installed"
  else
    ${PKG}_install libssh2
  fi
}

libssh2_status() {
  echo
}

libssh2_start() {
  echo
}

libssh2_stop() {
  echo
}

libssh2_restart() {
  echo
}
